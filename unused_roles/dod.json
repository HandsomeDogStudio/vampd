{
  "name": "dod",
  "description": "The role for the Deluxe OnDemand site",
  "chef_type": "role",
  "json_class": "Chef::Role",
  "default_attributes": {},
  "override_attributes": {
    "drupal": {
      "sites": {
        "dod": {
          "active": true,
          "deploy": {
            "action": ["deploy", "import"]
          },
          "drupal": {
            "settings": {
              "docroot": "docroot",
              "db_file": "/vagrant/chef/roles/dod/dod.sql"   
            }
          },
          "drupal_user": {
            "admin_user": "admin"
          },
          "repository": {
            "host": "svn-3453.devcloud.hosting.acquia.com",
            "uri": "deluxeondemand@svn-3453.devcloud.hosting.acquia.com:deluxeondemand.git",
            "revision": "prod"
          }
        }
      }
    }
  }
}